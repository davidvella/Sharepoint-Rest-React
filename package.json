{
  "name": "@davidvella/sharepoint-rest-react",
  "version": "0.2.9",
  "description": "Work in Progress. This library extends the Office Fabric-UI React library, to help create Office 365 components in SharePoint 2013/Online (Classic) environments. ",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "build": "rm -rf ./lib/* && tsc && npm run copyFiles && node_modules/.bin/typedoc --mode modules --tsconfig tsconfig.json --out docs src",
    "copyFiles": "copyfiles -u 1 src/**/*.scss src/**/*.png lib/"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidvella/sharepoint-rest-react"
  },
  "keywords": [
    "sharepoint",
    "sharepoint 2013",
    "sharepoint online",
    "react",
    "rest",
    "api",
    "typescript",
    "javascript"
  ],
  "author": "David Vella <david@vella.uk.com> (https://github.com/davidvella)",
  "license": "MIT",
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "chai": "^4.2.0",
    "core-js": "^2.6.4",
    "mocha": "^5.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
